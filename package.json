{
  "name": "calendera",
  "version": "1.0.0",
  "description": "AI-powered unified inbox for Gmail, Slack, Discord, iMessage, and more",
  "scripts": {
    "install:all": "cd frontend && npm install && cd ../backend && npm install",
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:backend:beeper": "cd backend && npm run beeper:dev",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm run start",
    "start:backend:beeper": "cd backend && npm run beeper",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start"
  },
  "keywords": ["ai", "inbox", "gmail", "slack", "discord", "claude", "calendar", "productivity"],
  "author": "Dean Shahin, Naisha Jain, Kevin Nguyen, Vamika Singhal",
  "license": "MIT"
}
